<MudSelect T="@string"
           Class="my-3"
           Label="@Label"
           Disabled="@(!IsEnabled)"
           MultiSelection="true"
           SelectedValues="SelectedItems"
           SelectedValuesChanged="@(values => OnSelectedItemsChanged(values))">
	@foreach (var item in Items)
	{
		<MudSelectItem T="string" Value="@item">@item</MudSelectItem>
	}
</MudSelect>